{
  "name": "api-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "build": "nest build",
    "build:webpack": "nest build --webpack",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/users/main",
    "deploy": "serverless deploy",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/users/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.12",
    "aws-sdk": "^2.1459.0",
    "axios": "^1.5.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "dynamoose": "^3.2.1",
    "express": "^4.18.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sqs-consumer": "^7.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@Feature/users(|/.*)$": "<rootDir>/libs/features/users/src/$1",
      "^@Feature/friendships(|/.*)$": "<rootDir>/libs/features/friendships/src/$1",
      "^@Feature/providers(|/.*)$": "<rootDir>/libs/features/providers/src/$1",
      "^@Feature/interests(|/.*)$": "<rootDir>/libs/features/interests/src/$1",
      "^@Feature/resources(|/.*)$": "<rootDir>/libs/features/resources/src/$1",
      "^@Feature/recommendations(|/.*)$": "<rootDir>/libs/features/recommendations/src/$1",
      "^@Provider/spotify-sdk(|/.*)$": "<rootDir>/libs/providers/spotify-sdk/src/$1",
      "^@Provider/queue(|/.*)$": "<rootDir>/libs/providers/queue/src/$1",
      "^@Provider/mailing(|/.*)$": "<rootDir>/libs/providers/mailing/src/$1",
      "^@Provider/storage(|/.*)$": "<rootDir>/libs/providers/storage/src/$1",
      "^@Provider/authentication(|/.*)$": "<rootDir>/libs/providers/authentication/src/$1"
    }
  }
}